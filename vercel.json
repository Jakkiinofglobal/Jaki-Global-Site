{
  "buildCommand": "vite build",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://jaki-global-site.onrender.com/api/:path*"
    }
  ],
  "routes": [
    {
      "src": "/((?!assets|favicon\\.ico).*)",
      "dest": "/index.html"
    }
  ]
}
